{
    "$schema": "vendor/infection/infection/resources/schema.json",
    "source": {
        "directories": [
            "src"
        ],
        "excludes": [
            "src/Infrastructure/Persistence/Doctrine/Migrations",
            "Infrastructure/Persistence/Doctrine/Migrations",
            "Infrastructure/Persistence/Doctrine/EntityManagerFactory.php",
            "src/Application.php",
            "Application.php"
        ]
    },
    "mutators": {
        "@default": true
    },
    "minMsi": 100,
    "minCoveredMsi": 100,
    "logs": {
        "html": "infection.html",
        "text": "infection.log"
    },
    "testFramework": "phpunit"
}