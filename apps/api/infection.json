{
    "$schema": "https://raw.githubusercontent.com/infection/infection/0.29/resources/schema.json",
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "text": "infection.log",
        "summary": "infection-summary.log",
        "html": "infection.html"
    },
    "mutators": "@default",
    "minMsi": 80,
    "minCoveredMsi": 85,
    "phpUnit": {
        "configDir": ".",
        "customPath": "vendor/bin/phpunit"
    }
}
